    <fragment
        android:name="com.streetsaarthi.screens.onboarding.splash.Splash"
        android:label="Splash"
        android:id="@+id/splash"
        tools:layout="@layout/splash">
        <action
            android:id="@+id/action_splash_to_login"
            app:destination="@id/loginPassword"
            app:launchSingleTop="true"
            app:popUpTo="@id/splash"
            app:popUpToInclusive="true"
            />

        <action
            android:id="@+id/action_splash_to_onboard"
            app:destination="@id/onboard"
            app:launchSingleTop="true"
            app:popUpTo="@id/onboard"
            app:popUpToInclusive="true"
            />

        <action
            android:id="@+id/action_splash_to_start"
            app:destination="@id/start"
            app:launchSingleTop="true"
            app:popUpTo="@id/splash"
            app:popUpToInclusive="true"
            />

        <action
            android:id="@+id/action_splash_to_home"
            app:destination="@id/home"
            app:launchSingleTop="true"
            app:popUpTo="@id/splash"
            app:popUpToInclusive="true"
            />

    </fragment>


    <fragment
        android:name="com.streetsaarthi.screens.onboarding.start.Start"
        android:label="Start"
        android:id="@+id/start"
        tools:layout="@layout/start">

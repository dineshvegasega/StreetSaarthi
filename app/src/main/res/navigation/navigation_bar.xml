            app:popUpTo="@id/walkThrough"
            app:popUpToInclusive="true"
            />

        <action
            android:id="@+id/action_walkThrough_to_Registar"
            app:destination="@id/register"
            app:launchSingleTop="true"
            app:popUpTo="@id/walkThrough"
            app:popUpToInclusive="true"
